                                <div class="form-group col-md-2">
                                    @*<div class="alert alert-info no-margin fade in">
                                            <code>Göndəriləcək ünvan </code>
                                        </div>*@
                                    <!-- widget content -->
                                    @Html.Action("FilterArchive", "Archive")
                                </div>
                                <div class="form-group col-md-10">
                                    @*<div class="alert alert-info no-margin fade in">
                                            <code>Göndəriləcək ünvan </code>
                                        </div>*@
                                    <!-- widget content -->

                                    <div id="docMainDiv">
                                    </div>

                                </div>
                            </div>

                        </div>
                    </div>

                </div>
            </div>
        </div>
    </article>
</div>
